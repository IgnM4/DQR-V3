<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.mycompany.drivequestrentals.interfaz.RegistroMantenimientoController">
    <children>
        <VBox spacing="10" layoutX="50" layoutY="30">
            <Label text="Registro de Mantenimiento"/>
            <TextField fx:id="txtId" promptText="ID Mantenimiento"/>
            <TextField fx:id="txtPatenteVehiculo" promptText="Patente Vehículo"/>
            <TextField fx:id="txtDescripcion" promptText="Descripción"/>
            <TextField fx:id="txtFecha" promptText="Fecha (AAAA-MM-DD)"/>
            <TextField fx:id="txtCosto" promptText="Costo"/>
            <TextField fx:id="txtKilometraje" promptText="Kilometraje"/>
            <Button text="Registrar" onAction="#registrarMantenimiento"/>
            <Button text="Volver" onAction="#volverAlMenu"/>
        </VBox>
    </children>
</AnchorPane>
